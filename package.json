{
	"name": "@equinor/sparql-graph",
	"version": "0.6.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/equinor/sparql-graph"
	},
	"author": {
		"name": "Dzianis Makeichyk",
		"email": "dzm@equinor.com"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"license": "MIT",
	"scripts": {
		"test": "react-scripts test",
		"lint": "eslint --ext .ts --ext .tsx src",
		"ts:testcompile": "tsc --noEmit",
		"dev": "npm run storybook",
		"storybook": "start-storybook -p 3000",
		"build:storybook": "build-storybook",
		"build:lib": "rollup -c --environment NODE_ENV:production",
		"prettier:check": "prettier --check .",
		"prettier:write": "prettier --write .",
		"prepublishOnly": "npm run build:lib",
		"checkcode": "npm run lint && npm run ts:testcompile && npm run prettier:check",
		"bump:patch": "npm --no-git-tag-version version patch",
		"bump:minor": "npm --no-git-tag-version version minor",
		"bump:major": "npm --no-git-tag-version version major",
		"prepare": "husky install"
	},
	"engines": {
		"npm": ">= 8.3.1",
		"node": ">= 16.14.0"
	},
	"main": "build/index.cjs.js",
	"types": "build/types/index.d.ts",
	"module": "build/index.es.js",
	"files": [
		"/build"
	],
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			},
			{
				"files": [
					"**/*/*.*"
				],
				"rules": {
					"react/no-unknown-property": 2,
					"react-hooks/rules-of-hooks": "off",
					"react-hooks/exhaustive-deps": "off"
				}
			}
		]
	},
	"browser": {
		"fs": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"peerDependencies": {
		"react": "^16.14.0 || ^17.0.0",
		"react-dom": "^16.14.0 || ^17.0.0"
	},
	"dependencies": {
		"@equinor/eds-core-react": "^0.20.4",
		"@rdfjs/types": "^1.1.0",
		"@rollup/plugin-json": "^4.1.0",
		"@types/jest": "^28.1.3",
		"@types/n3": "^1.10.4",
		"@types/node": "^18.0.0",
		"@types/react": "^18.0.1",
		"@types/react-cytoscapejs": "^1.2.2",
		"@types/react-dom": "^18.0.0",
		"@types/three": "^0.141.0",
		"3d-force-graph": "^1.70.10",
		"bluebird": "^3.7.2",
		"chalk": "^2.4.1",
		"chalk-rainbow": "^1.0.0",
		"cytoscape": "^3.21.1",
		"cytoscape-cola": "^2.5.1",
		"cytoscape-cose-bilkent": "^4.1.0",
		"cytoscape-dagre": "^2.4.0",
		"lint-staged": "^13.0.3",
		"md5": "^2.3.0",
		"n3": "^1.16.2",
		"nanoid": "^4.0.0",
		"react-cytoscapejs": "^1.2.1",
		"react-force-graph": "^1.41.12",
		"rollup": "^2.75.6",
		"rollup-plugin-ignore": "^1.0.10",
		"rollup-plugin-svg": "^2.0.0",
		"sass": "^1.52.2",
		"sparqljs": "^3.5.1",
		"three": "^0.141.0",
		"three-css2drender": "^1.0.0",
		"three-spritetext": "^1.6.5",
		"typescript": "^4.6.4"
	},
	"devDependencies": {
		"@babel/plugin-proposal-export-default-from": "^7.17.12",
		"@babel/plugin-proposal-export-namespace-from": "^7.16.7",
		"@babel/plugin-transform-runtime": "^7.17.10",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-typescript": "^8.3.2",
		"@storybook/addon-actions": "^6.4.22",
		"@storybook/addon-essentials": "^6.5.7",
		"@storybook/addon-links": "^6.5.7",
		"@storybook/node-logger": "^6.4.21",
		"@storybook/preset-create-react-app": "^3.2.0",
		"@storybook/react": "^6.5.7",
		"@testing-library/dom": "^8.16.0",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.5",
		"babel-loader": "^8.2.5",
		"husky": "^8.0.1",
		"prettier": "^2.6.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "4.0.3",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-sass": "^1.2.10",
		"storybook-addon-state": "^1.0.3",
		"storybook-addon-theme-playground": "^2.1.0"
	}
}
