{
	"name": "@equinor/sparql-graph",
	"version": "0.2.4",
	"repository": {
		"type": "git",
		"url": "https://github.com/equinor/sparql-graph"
	},
	"author": {
		"name": "Dzianis Makeichyk",
		"email": "dzm@equinor.com"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"license": "MIT",
	"scripts": {
		"test": "react-scripts test",
		"lint": "eslint --ext .ts --ext .tsx src",
		"ts:testcompile": "tsc --noEmit",
		"dev": "npm run storybook",
		"storybook": "start-storybook -p 3000",
		"build:storybook": "build-storybook",
		"build:lib": "rollup -c --environment NODE_ENV:production",
		"prettier:check": "prettier --check .",
		"prettier:write": "prettier --write .",
		"prepublishOnly": "npm run build:lib",
		"checkcode": "npm run lint && npm run ts:testcompile && npm run prettier:check",
		"bump:patch": "npm --no-git-tag-version version patch",
		"bump:minor": "npm --no-git-tag-version version minor",
		"bump:major": "npm --no-git-tag-version version major",
		"prepare": "husky install"
	},
	"engines": {
		"npm": ">= 8.3.1",
		"node": ">= 16.14.0"
	},
	"main": "build/index.cjs.js",
	"types": "build/types/index.d.ts",
	"module": "build/index.es.js",
	"files": [
		"/build",
		"/docs"
	],
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			},
			{
				"files": [
					"**/*/*.*"
				],
				"rules": {
					"react/no-unknown-property": 2,
					"react-hooks/rules-of-hooks": "off",
					"react-hooks/exhaustive-deps": "off"
				}
			}
		]
	},
	"browser": {
		"fs": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"peerDependencies": {
		"react": "^16.14.0 || ^17.0.0",
		"react-dom": "^16.14.0 || ^17.0.0"
	},
	"dependencies": {
		"@azure/msal-browser": "^2.22.1",
		"@azure/msal-react": "^1.3.1",
		"@equinor/eds-core-react": "^0.18.0",
		"@rdfjs/types": "^1.1.0",
		"@rollup/plugin-json": "^4.1.0",
		"@types/jest": "^27.0.3",
		"@types/n3": "^1.10.4",
		"@types/node": "^17.0.23",
		"@types/react": "^17.0.37",
		"@types/react-cytoscapejs": "^1.2.2",
		"@types/react-dom": "^17.0.11",
		"@types/react-transition-group": "^4.4.3",
		"bluebird": "^3.7.2",
		"chalk": "^2.4.1",
		"chalk-rainbow": "^1.0.0",
		"cytoscape": "^3.21.0",
		"cytoscape-cola": "^2.5.1",
		"cytoscape-cose-bilkent": "^4.1.0",
		"cytoscape-dagre": "^2.4.0",
		"lint-staged": "^12.0.3",
		"md5": "^2.3.0",
		"n3": "^1.15.0",
		"rdfstore": "^0.9.17",
		"react-cytoscapejs": "^1.2.1",
		"react-transition-group": "^4.4.2",
		"rollup": "^2.60.2",
		"rollup-plugin-scss": "3",
		"rollup-plugin-svg": "^2.0.0",
		"sass": "^1.49.9",
		"sparqljs": "^3.5.1",
		"styled-components": "^5.3.3",
		"typescript": "^4.6.3"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^21.0.0",
		"@rollup/plugin-node-resolve": "^13.0.5",
		"@rollup/plugin-typescript": "^8.3.0",
		"@storybook/addon-actions": "^6.4.9",
		"@storybook/addon-essentials": "^6.4.9",
		"@storybook/addon-links": "^6.4.9",
		"@storybook/node-logger": "^6.4.9",
		"@storybook/preset-create-react-app": "^3.2.0",
		"@storybook/react": "^6.4.9",
		"@testing-library/dom": "^8.11.4",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.4.1",
		"husky": "^7.0.0",
		"prettier": "^2.6.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "4.0.3",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"storybook-addon-theme-playground": "^2.1.0"
	}
}
