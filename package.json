		"@types/react-cytoscapejs": "^1.2.2",
		"@types/react-dom": "^18.0.6",
		"@types/three": "^0.146.0",
		"@vitejs/plugin-react": "^2.1.0",
		"eslint": "^8.23.0",
		"eslint-config-react-app": "^7.0.1",
		"gojs": "^2.2.16",
		"gojs-react": "^1.1.1",
		"jsdom": "^20.0.0",
		"prettier": "^2.6.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"rollup-plugin-peer-deps-external": "^2.2.4",
