{
	"name": "@equinor/rdf-graph",
	"version": "0.7.5",
	"repository": {
		"type": "git",
		"url": "https://github.com/equinor/rdf-graph"
	},
	"author": {
		"name": "Dzianis Makeichyk",
		"email": "dzm@equinor.com"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"contributors": [
		{
			"name": "Lorentz Fjellanger Barstad",
			"email": "loba@equinor.com"
		},
		{
			"name": "Martin Tofteberg Ulvesæter",
			"email": "mulv@equinor.com"
		},
		{
			"name": "Eirik Nordstrand",
			"email": "einor@equinor.com"
		},
		{
			"name": "Arne Måge",
			"email": "arnm@equinor.com"
		}
	],
	"license": "MIT",
	"scripts": {
		"test": "react-scripts test",
		"lint": "eslint --ext .ts --ext .tsx src",
		"ts:testcompile": "tsc --noEmit",
		"dev": "npm run storybook",
		"storybook": "start-storybook -p 3000",
		"build:storybook": "build-storybook",
		"build:lib": "rollup -c --environment NODE_ENV:production",
		"prettier:check": "prettier --check .",
		"prettier:write": "prettier --write .",
		"prepublishOnly": "npm run build:lib",
		"checkcode": "npm run lint && npm run ts:testcompile && npm run prettier:check",
		"bump:patch": "npm --no-git-tag-version version patch",
		"bump:minor": "npm --no-git-tag-version version minor",
		"bump:major": "npm --no-git-tag-version version major",
		"prepare": "husky install"
	},
	"engines": {
		"npm": ">= 8.3.1",
		"node": ">= 16.14.0"
	},
	"main": "dist/index.cjs.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.esm.js",
	"files": [
		"/dist"
	],
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			},
			{
				"files": [
					"**/*/*.*"
				],
				"rules": {
					"react/no-unknown-property": 2,
					"react-hooks/rules-of-hooks": "off",
					"react-hooks/exhaustive-deps": "off",
					"no-throw-literal": 0,
					"@typescript-eslint/no-unused-vars": [
						"warn",
						{
							"argsIgnorePattern": "^_",
							"varsIgnorePattern": "^_",
							"caughtErrorsIgnorePattern": "^_"
						}
					]
				}
			}
		]
	},
	"browser": {
		"fs": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"peerDependencies": {
		"react": "^16.14.0 || ^17.0.0",
		"react-dom": "^16.14.0 || ^17.0.0"
	},
	"dependencies": {
		"@equinor/eds-core-react": "^0.20.4",
		"3d-force-graph": "^1.70.10",
		"cytoscape": "^3.22.1",
		"cytoscape-cola": "^2.5.1",
		"cytoscape-cose-bilkent": "^4.1.0",
		"cytoscape-dagre": "^2.4.0",
		"n3": "^1.16.2",
		"react-cytoscapejs": "^1.2.1",
		"react-force-graph": "^1.41.12",
		"three": "^0.143.0"
	},
	"devDependencies": {
		"@rdfjs/types": "^1.1.0",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-typescript": "^8.3.2",
		"@storybook/addon-essentials": "^6.5.7",
		"@storybook/addon-links": "^6.5.7",
		"@storybook/preset-create-react-app": "^3.2.0",
		"@storybook/react": "^6.5.7",
		"@testing-library/dom": "^8.16.1",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.5",
		"@types/jest": "^28.1.6",
		"@types/n3": "^1.10.4",
		"@types/node": "^18.6.4",
		"@types/react": "^18.0.15",
		"@types/react-cytoscapejs": "^1.2.2",
		"@types/react-dom": "^18.0.6",
		"@types/three": "^0.143.0",
		"babel-loader": "^8.2.5",
		"gojs": "^2.2.12",
		"gojs-react": "^1.1.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.6.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "4.0.3",
		"rollup": "^2.77.3",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-sass": "^1.2.10",
		"rollup-plugin-sizes": "^1.0.4",
		"rollup-plugin-svg": "^2.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"storybook-addon-state": "^1.0.3",
		"storybook-addon-theme-playground": "^2.1.0",
		"typescript": "^4.7.4"
	}
}
