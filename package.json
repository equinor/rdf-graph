		"cytoscape-dagre": "^2.4.0",
		"n3": "^1.16.2",
		"react-cytoscapejs": "^2.0.0",
		"react-force-graph": "^1.41.14",
		"three": "^0.146.0"
	},
	"devDependencies": {
		"@equinor/eds-core-react": "^0.24.0",
		"@evilmartians/lefthook": "^1.1.3",
		"@rdfjs/types": "^1.1.0",
