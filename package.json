	"scripts": {
		"test": "vitest",
		"lint": "eslint --ext .ts --ext .tsx src",
		"ts:compileTest": "tsc --noEmit",
		"dev": "npm run storybook",
		"storybook": "start-storybook -p 3000",
		"clean:dist": "rimraf dist/",
		"build:storybook": "build-storybook",
		"build:lib": "npm run clean:dist && tsc --project tsconfig.build.json",
		"prettier:check": "prettier --check .",
		"prettier:write": "prettier --write .",
		"prepublishOnly": "npm run build:lib",
		"validateCode": "npm run lint && npm run ts:compileTest && npm run prettier:check"
	},
	"engines": {
		"npm": ">= 8.3.1",
		"node": ">= 16.14.0"
	},
	"main": "dist/main.js",
	"module": "dist/main.js",
	"files": [
		"dist",
		"src",
		"CHANGELOG.md"
	],
	"browser": {
		"fs": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"peerDependencies": {
		"gojs": ">=2.2.16",
		"react": "^16.14.0 || ^17.0.0",
